# Appwrite Configuration
# Copy this file to .env and fill in your actual values

EXPO_PUBLIC_APPWRITE_ENDPOINT=https://your-appwrite-endpoint.io/v1
EXPO_PUBLIC_APPWRITE_PLATFORM=com.yourcompany.yourapp
EXPO_PUBLIC_APPWRITE_PROJECT_ID=your_project_id
EXPO_PUBLIC_APPWRITE_STORAGE_ID=your_storage_bucket_id
EXPO_PUBLIC_APPWRITE_DATABASE_ID=your_database_id
EXPO_PUBLIC_APPWRITE_USER_COLLECTION_ID=your_user_collection_id
EXPO_PUBLIC_APPWRITE_VIDEO_COLLECTION_ID=your_video_collection_id
EXPO_PUBLIC_APPWRITE_EVENTS_COLLECTION_ID=your_events_collection_id
EXPO_PUBLIC_APPWRITE_NOTIFICATIONS_COLLECTION_ID=your_notifications_collection_id

# App Configuration
EXPO_PUBLIC_APP_NAME=Your App Name
EXPO_PUBLIC_APP_VERSION=1.0.0

# Development Settings
EXPO_PUBLIC_DEBUG_MODE=true
EXPO_PUBLIC_LOG_LEVEL=debug

# Push Notifications (Optional)
# EXPO_PUBLIC_PUSH_NOTIFICATION_KEY=your_expo_push_key
# EXPO_PUBLIC_FCM_SERVER_KEY=your_firebase_server_key

# Deep Linking
EXPO_PUBLIC_DEEP_LINK_SCHEME=yourapp

# Additional Security Settings (Optional)
# EXPO_PUBLIC_API_TIMEOUT=30000
# EXPO_PUBLIC_MAX_FILE_SIZE=50000000
# EXPO_PUBLIC_ALLOWED_FILE_TYPES=video/mp4,image/jpeg,image/png
